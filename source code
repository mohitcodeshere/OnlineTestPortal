    private void submitActionPerformed(java.awt.event.ActionEvent evt) {                                       
jPanel1.setVisible(true); //showing table below                    
        DefaultTableModel model = (DefaultTableModel) jTable3.getModel();
		
        //----------------pi--------------------------
        if(value.getText().equals("3.14")){
            model.addRow(new Object[]{1,value.getText(),"3.14", 1});//showing in the table
            score++;//incrementing the score in the table
        }
        else{
            model.addRow(new Object[]{1,value.getText(),"3.14", 0});
        }
        //------------------radiobutton-------------------------
        if(jRadioButton1.isSelected()){
            model.addRow(new Object[]{2,jRadioButton1.getText(), "4*sides", 1});
            score++;
        }
        else if(jRadioButton2.isSelected()){
            model.addRow(new Object[]{2,jRadioButton2.getText(), "4*sides", 0});
        }
        else{
            model.addRow(new Object[]{2," ", "4*sides", 0});//for leaving the space blank
        }
          
        //-------------combobox----------------------------
        String s=jComboBox1.getSelectedItem().toString(); 
        if(s=="1"){
            model.addRow(new Object[]{3,jComboBox1.getSelectedItem(), "1", 1});
            score++;
        }
        else{
        model.addRow(new Object[]{3,jComboBox1.getSelectedItem(), "1", 0});
        }
        //------------checkbox--------------------------------
        
        if(jCheckBox4.isSelected()){
        model.addRow(new Object[]{4,jCheckBox4.getText(), "Convex Polygon, Cyclic", 0});
        }
        else if(jCheckBox1.isSelected() && jCheckBox2.isSelected()){
        model.addRow(new Object[]{4,jCheckBox1.getText(), "Convex Polygon, Cyclic",1});
        score++;
        }
        else if(jCheckBox1.isSelected()){
        model.addRow(new Object[]{4,jCheckBox1.getText(), "Convex Polygon, Cyclic",0});
        }
        else if(jCheckBox2.isSelected()){
        model.addRow(new Object[]{4,jCheckBox2.getText(), "Convex Polygon, Cyclic",0});
        }
        else if(jCheckBox3.isSelected()){
            model.addRow(new Object[]{4,jCheckBox3.getText(), "Convex Polygon, Cyclic", 0});
        }
        else{
        model.addRow(new Object[]{4,jCheckBox4.getText(), "Convex Polygon, Cyclic", 0});
        }
        //---------------------how many digits of 1000? --------------------------
        if(B3=="4"){
            model.addRow(new Object[]{5,B3, "4", 1});
            score++;
        }
        else if(B3=="3"){
            model.addRow(new Object[]{5,B3, "4", 0});
        }
        else {
            model.addRow(new Object[]{5,B3, "4", 0});
        }
        //------------To Display Message Box----------------------
      JOptionPane.showMessageDialog(null,name.getText()+ " your score is"+" "+score+"/5.");
      score=0;        // TODO add your handling code here:
    }                                      

