    private void nameKeyTyped(java.awt.event.KeyEvent evt) {                              
 char c=evt.getKeyChar();
        if(Character.isDigit(c)){
            evt.consume();}
    }                             
//>>>>>>>>>>>>>>> Validations on Phone Number >>>>>>>>>>>>>>>>
    private void phoneKeyReleased(java.awt.event.KeyEvent evt) {                                  
  String mobileNo= phone.getText();// storing mobile number in a variable mobileNo
        if(mobileNo.matches("^[0-9]*$") && mobileNo.length()==10) //use 0 to 9 numbers and lenght should be equals to 10 
        {
            phone.setBackground(Color.green);
           
           
        }
        else
            phone.setBackground(Color.red);        // TODO add your handling code here:
    }                                 
//-----------Validations in PI value------------------------
    private void valueKeyReleased(java.awt.event.KeyEvent evt) {                                  
      String Q1= value.getText();// storing mobile number in a variable mobileNo
        if(Q1.matches("^[0-9.]*$") && Q1.length()>0 )
        {
            value.setBackground(Color.green);
           //Q1Text.setEditable(true);
           
        }
        else{
            value.setBackground(Color.red);
             //Q1Text.setEditable(false);
    }  // TODO add your handling code here:
    }                                 

